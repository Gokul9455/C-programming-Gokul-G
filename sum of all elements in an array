#include <stdio.h>

int main() {
    int n,i;
    int arr[100];
    int sum=0;
    printf("Enter number of elements:");
    scanf("%d",&n);
    printf("Enter %d elements:\n",n);
    for(i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    for(i=0;i<n;i++){
        sum=sum+arr[i];
    }
    printf("sum of all elements=%d",sum);

    return 0;
}
