
#include <stdio.h>

    int findmax(int arr[],int n){
        int max=arr[0];
        for(int i=1;i<n;i++){
        if(arr[i]>max){
            max=arr[i];
        }
    }
    return max;
}
int main(){
    int arr[]={10,20,34,67,101};
    int n=sizeof(arr)/sizeof(arr[0]);
    int max=findmax(arr,n);
    printf("maximum element: %d\n",max);
    return 0;
}
