
#include <stdio.h>

    int findmin(int arr[],int n){
        int min=arr[0];
        for(int i=1;i>n;i--){
        if(arr[i]>min){
            min=arr[i];
        }
    }
    return min;
}
int main(){
    int arr[]={10,20,34,67,101};
    int n=sizeof(arr)/sizeof(arr[0]);
    int min=findmin(arr,n);
    printf("minimum element: %d\n",min);
    return 0;
}
